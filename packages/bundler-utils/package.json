{
  "name": "@umijs/bundler-utils",
  "version": "4.0.0-alpha.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "compiled"
  ],
  "scripts": {
    "build": "pnpm tsc",
    "build:deps": "pnpm esno ../../scripts/bundleDeps.ts",
    "dev": "pnpm build -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umijs/umi-next"
  },
  "authors": [
    "chencheng <sorrycc@gmail.com> (https://github.com/sorrycc)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/umijs/umi-next/issues",
  "homepage": "https://github.com/umijs/umi-next/tree/master/packages/bundler-utils#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@umijs/utils": "4.0.0-alpha.1",
    "esbuild": "0.13.8"
  },
  "devDependencies": {
    "@babel/code-frame": "7.15.8",
    "@babel/core": "7.15.8",
    "@babel/generator": "7.15.8",
    "@babel/parser": "7.15.8",
    "@babel/plugin-proposal-decorators": "7.15.8",
    "@babel/plugin-proposal-do-expressions": "7.14.5",
    "@babel/plugin-proposal-export-default-from": "7.14.5",
    "@babel/plugin-proposal-export-namespace-from": "7.14.5",
    "@babel/plugin-proposal-function-bind": "7.14.5",
    "@babel/plugin-proposal-partial-application": "7.14.5",
    "@babel/plugin-proposal-pipeline-operator": "7.15.0",
    "@babel/plugin-proposal-record-and-tuple": "7.14.5",
    "@babel/plugin-transform-runtime": "7.15.8",
    "@babel/preset-env": "7.15.8",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "7.15.0",
    "@babel/register": "7.15.3",
    "@babel/template": "7.15.4",
    "@babel/traverse": "7.15.4",
    "@babel/types": "7.15.6",
    "@types/babel__code-frame": "7.0.3",
    "@types/babel__generator": "7.6.3",
    "@types/babel__parser": "7.1.1",
    "@types/babel__template": "7.4.1",
    "@types/babel__traverse": "7.14.2",
    "cjs-module-lexer": "1.2.2",
    "es-module-lexer": "0.9.3"
  },
  "compiledConfig": {
    "deps": [
      "cjs-module-lexer",
      "es-module-lexer",
      "esbuild",
      "./bundles/babel/bundle"
    ],
    "externals": {
      "cjs-module-lexer": "$$LOCAL",
      "debug": "@umijs/utils/compiled/debug",
      "esbuild": "$$LOCAL",
      "es-module-lexer": "$$LOCAL"
    },
    "noMinify": [
      "./bundles/babel/bundle"
    ],
    "extraDtsDeps": [
      "@babel/code-frame",
      "@babel/core",
      "@babel/generator",
      "@babel/parser",
      "@babel/template",
      "@babel/traverse",
      "@babel/types"
    ]
  }
}
