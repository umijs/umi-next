{
  "name": "@umijs/bundler-webpack",
  "version": "4.0.0-alpha.1",
  "description": "@umijs/bundler-webpack",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "bundler-webpack": "bin/bundler-webpack.js"
  },
  "files": [
    "bin",
    "compiled",
    "dist"
  ],
  "scripts": {
    "build": "pnpm tsc",
    "build:deps": "pnpm esno ../../scripts/bundleDeps.ts",
    "dev": "pnpm build -- --watch",
    "generate:webpackPackages": "zx ./scripts/generateWebpackPackages.mjs"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/umijs/umi-next"
  },
  "authors": [
    "chencheng <sorrycc@gmail.com> (https://github.com/sorrycc)"
  ],
  "license": "MIT",
  "bugs": "https://github.com/umijs/umi-next/issues",
  "homepage": "https://github.com/umijs/umi-next/tree/master/packages/bundler-webpack#readme",
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@umijs/babel-preset-umi": "4.0.0-alpha.1",
    "@umijs/utils": "4.0.0-alpha.1",
    "es5-imcompatible-versions": "^0.1.73",
    "jest-worker": "27.1.0"
  },
  "devDependencies": {
    "@types/webpack-sources": "3.2.0",
    "babel-loader": "8.2.2",
    "schema-utils": "3.1.1",
    "tapable": "2.2.0",
    "terser": "5.7.2",
    "terser-webpack-plugin": "5.2.0",
    "webpack": "5.51.1",
    "webpack-5-chain": "8.0.0",
    "webpack-sources": "3.2.0"
  },
  "compiledConfig": {
    "deps": [
      "babel-loader",
      "schema-utils",
      "tapable",
      "terser",
      "terser-webpack-plugin",
      "webpack-5-chain",
      "webpack-sources",
      "./bundles/webpack/bundle"
    ],
    "externals": {
      "@swc/core": "@swc/core",
      "@babel/core": "@umijs/bundler-utils/compiled/babel/core",
      "es-module-lexer": "@umijs/bundler-utils/compiled/es-module-lexer",
      "esbuild": "@umijs/bundler-utils/compiled/esbuild",
      "jest-worker": "jest-worker",
      "schema-utils": "$$LOCAL",
      "tapable": "$$LOCAL",
      "terser": "$$LOCAL",
      "terser-webpack-plugin": "$$LOCAL",
      "uglify-js": "uglify-js",
      "webpack-5-chain": "$$LOCAL",
      "webpack-sources": "$$LOCAL"
    },
    "noMinify": [
      "terser-webpack-plugin",
      "./bundles/webpack/bundle"
    ],
    "extraDtsDeps": [
      "webpack"
    ],
    "extraDtsExternals": [
      "@swc/core",
      "esbuild",
      "uglify-js",
      "webpack"
    ],
    "excludeDtsDeps": [
      "babel-loader"
    ]
  }
}
